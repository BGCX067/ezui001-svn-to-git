<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >

<sqlMap namespace="ContentProvider">

<!-- Select CONTENT_PROVIDER : 取出CP_TITLE資料 -->
<select id="getProviderTitle" resultClass="java.lang.String">
select CP_TITLE from CONTENT_PROVIDER where CP_ID = #cpId# 
</select>

</sqlMap>
